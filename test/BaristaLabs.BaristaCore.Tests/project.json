{
  "version": "1.0.0-*",

  "buildOptions": {
    "debugType": "portable",
    "copyToOutput": {
      "include": [ "xunit.runner.json" ],
      "mappings": {
        "ChakraCore/": "../../lib/ChakraCore/**/*"
      }
    }
  },
  "dependencies": {
    "BaristaLabs.BaristaCore.Common": "1.0.0-*",
    "System.Runtime.Serialization.Primitives": "4.3.0",
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-build10015",
    "Microsoft.CodeCoverage": "1.0.2",
    "coveralls.net": "0.7.0"
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": [
        "dotnet5.4",
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  }
}
